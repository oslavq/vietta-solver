    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in library 'vietta_solver'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--lib",
                    "--package=vietta-solver"
                ],
                "filter": {
                    "name": "vietta_solver",
                    "kind": "lib"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'vietta-solver'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=vietta-solver",
                    "--package=vietta-solver"
                ],
                "filter": {
                    "name": "vietta-solver",
                    "kind": "bin"
                }
            },
            "args": ["-28", "96"],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
